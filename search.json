---
layout: nil
---
[
  {% for chart in site.charts %}
    {
      "title"    : "{{ chart.title | escape }}",
      "description": "{{ chart.description }}",
      "category" : "{{ chart.category }}",
      "url"      : "{{ site.baseurl }}{{ chart.url }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]